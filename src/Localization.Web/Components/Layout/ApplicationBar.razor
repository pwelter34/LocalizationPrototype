

<header class="d-print-none">
    <nav id="navbar-main"
         class="navbar navbar-expand-lg navbar-light navbar-default fixed-top shadow-sm">

        <div class="container-fluid">
            <a class="navbar-brand p-0"
               href="@RouteLinks.Home()">
                <img src="/logo/ReadyCredit.png"
                     class="me-2"
                     height="30" />
                <LocalizedText Key="@Localizations.Navigation.Home">
                    Localization
                </LocalizedText>
            </a>

            <button class="navbar-toggler"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#navbar-main-menu"
                    aria-controls="navbar-main-menu"
                    aria-expanded="false"
                    aria-label="Toggle Navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse"
                 id="navbar-main-menu">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <NavLink ActiveClass="active"
                                 Match="NavLinkMatch.Prefix"
                                 href="@RouteLinks.Faq()"
                                 class="nav-link">
                            <LocalizedText Key="@Localizations.Navigation.Faq">
                                FAQ
                            </LocalizedText>
                        </NavLink>
                    </li>
                    <li class="nav-item">
                        <NavLink ActiveClass="active"
                                 Match="NavLinkMatch.Prefix"
                                 href="@RouteLinks.Privacy()"
                                 class="nav-link">
                            <LocalizedText Key="@Localizations.Navigation.Privacy">
                                Privacy
                            </LocalizedText>
                        </NavLink>
                    </li>
                </ul>


                <ul class="navbar-nav me-4">
                    <LanguageMenu />
                </ul>
            </div>
        </div>
    </nav>
</header>

